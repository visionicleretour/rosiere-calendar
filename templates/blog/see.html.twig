{% block body %}

    <link rel="stylesheet" href="/assets/app/blog.css">

    {# Navbar #}
    {% include 'partials/navbar.html.twig' %}

    <div class="blog">
        <div class="blog-content-wrapper">
            <div class="blog-content">
                <div style="display: flex; justify-content: center">
                    <img src="data:image/jpeg;base64," alt="Image de l'article" class="blog-image">
                </div>
                <h3 class="blog-title">{{ article.title }}</h3>
                <p class="blog-body">{{ article.content|raw }}</p>
                <span class="blog-date">Crée le {{article.createdAt|date("d/m/Y")}} </span>
            </div>
        </div>
    </div>
{% endblock %}
